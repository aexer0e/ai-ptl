{
    "format_version": "1.10.0",
    "particle_effect": {
        "description": {
            "identifier": "ns_ptl:void_tendrils",
            "basic_render_parameters": { "material": "particles_blend", "texture": "textures/particle/particles" }
        },
        "components": {
            "minecraft:emitter_rate_steady": { "spawn_rate": "variable.emitter_density * 3", "max_particles": 60 },
            "minecraft:emitter_lifetime_looping": { "active_time": 1 },
            "minecraft:emitter_shape_box": {
                "offset": [
                    "variable.emitter_spread * (Math.random(-0.5, 0.5))",
                    0,
                    "variable.emitter_spread * (Math.random(-0.5, 0.5))"
                ],
                "half_dimensions": [ 0.3, 0.02, 0.3 ],
                "direction": [ 0, 1, 0 ]
            },
            "minecraft:particle_initial_speed": "Math.random(0.5, 1.5)",
            "minecraft:particle_initial_spin": {
                "rotation": "Math.random(0, 360)",
                "rotation_rate": "Math.random(-30, 30)"
            },
            "minecraft:particle_lifetime_expression": { "max_lifetime": "Math.random(1.5, 3.0)" },
            "minecraft:particle_motion_parametric": {
                "relative_position": [
                    "Math.sin(variable.particle_age * 3 + variable.particle_random_1 * 6.28) * 0.2 * variable.particle_age",
                    "variable.particle_age * 0.8",
                    "Math.cos(variable.particle_age * 3 + variable.particle_random_2 * 6.28) * 0.2 * variable.particle_age"
                ]
            },
            "minecraft:particle_appearance_billboard": {
                "size": [ "0.1 - variable.particle_age * 0.02", "0.1 - variable.particle_age * 0.02" ],
                "facing_camera_mode": "lookat_xyz",
                "uv": { "texture_width": 128, "texture_height": 128, "uv": [ 0, 24 ], "uv_size": [ 8, 8 ]}
            },
            "minecraft:particle_appearance_tinting": { "color": [ 0.3, 0.1, 0.5, "0.7 - variable.particle_age * 0.2" ]}
        }
    }
}