                    "v.base = query.property('gm1_sky:base');",
                    "v.pants = query.property('gm1_sky:pants');",
                    "v.shirt = query.property('gm1_sky:shirt');",
                    "v.use_material = 0;",
                    "v.spawn_time = 0;",
                    "v.seconds_till_color = 3.0;",
                    "v.ticks_until_transparent = 20;",
                    "v.light_colour_multi = 0;",
                    "v.starting_light_colour_multi = 5;"
                ],
                "pre_animation": [
                    "variable.tcos0 = (math.cos(query.modified_distance_moved * 38.17) * query.modified_move_speed / variable.gliding_speed_value) * 57.3;",
                    "variable.attack_body_rot_y = Math.sin(360*Math.sqrt(variable.attack_time)) * 5.0;",
                    "v.bodytype = query.property('gm1_sky:bodytype');",
                    "v.base = query.property('gm1_sky:base');",
                    "v.pants = query.property('gm1_sky:pants');",
                    "v.shirt = query.property('gm1_sky:shirt');",
                    "v.ticks_until_transparent = 20*v.seconds_till_color;",
                    "v.spawn_time = query.property('gm1_sky:spawn_sequence') > 0 ? v.spawn_time*1 + q.delta_time : 0;",
                    "v.light_colour_multi = v.spawn_time < v.seconds_till_color ? v.light_colour_multi + (v.starting_light_colour_multi/v.ticks_until_transparent) : 0;",
                    "v.use_material = query.property('gm1_sky:spawn_sequence') > 0 && v.spawn_time < v.seconds_till_color ? 1 : 0;"
                ],
                "animate": [
                    "root",
                    "aiplayer_controller",
                    "aiplayer_punching_controller",
                    "aiplayer_spawn_controller",
                    "ban_controller",
                    "aiplayer_bow_controller",
