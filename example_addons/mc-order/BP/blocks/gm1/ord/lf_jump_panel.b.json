{
    "format_version": "1.21.20",
    "minecraft:block": {
        "description": {
            "identifier": "gm1_ord:lf_jump_panel",
            "states": {
                "gm1_ord:spring_entity": [ false, true ]
            },
            "menu_category": { "category": "construction", "is_hidden_in_commands": false },
            "traits": { "minecraft:placement_direction": { "enabled_states": [ "minecraft:cardinal_direction" ]}}
        },
        "components": {
            "minecraft:display_name": "tile.gm1_ord:lf_jump_panel.name",
            "minecraft:destructible_by_mining": true,
            "minecraft:destructible_by_explosion": false,
            "minecraft:friction": 0.4,
            "minecraft:map_color": "#e7391e",
            "minecraft:material_instances": { "*": { "texture": "gm1_ord_lf_jump_panel" }},
            "minecraft:geometry": "geometry.gm1_ord.lf_jump_panel",
            "minecraft:collision_box": { "origin": [ -8, 0, -8 ], "size": [ 16, 2, 16 ]},
            "minecraft:selection_box": { "origin": [ -8, 0, -8 ], "size": [ 16, 4, 16 ]},
            "minecraft:custom_components": [ "gm1_ord:springable_block_entity" ],
            "minecraft:tick": { "interval_range": [ 0, 0 ], "looping": true }
        },
        "permutations": [
            {
                "condition": "q.block_state('minecraft:cardinal_direction') == 'north'",
                "components": {
                    "minecraft:transformation": { "rotation": [ 0, 0, 0 ]}
                }
            },
            {
                "condition": "q.block_state('minecraft:cardinal_direction') == 'west'",
                "components": {
                    "minecraft:transformation": { "rotation": [ 0, -90, 0 ]}
                }
            },
            {
                "condition": "q.block_state('minecraft:cardinal_direction') == 'south'",
                "components": {
                    "minecraft:transformation": { "rotation": [ 0, 180, 0 ]}
                }
            },
            {
                "condition": "q.block_state('minecraft:cardinal_direction') == 'east'",
                "components": {
                    "minecraft:transformation": { "rotation": [ 0, 90, 0 ]}
                }
            },
            {
                "condition": "q.block_state('gm1_ord:spring_entity') == true",
                "components": {
                    "minecraft:geometry": {
                        "identifier": "geometry.gm1_ord.lf_jump_panel",
                        "bone_visibility": { "root": false, "spring": false, "spring_platform": false }
                    },
                    "minecraft:material_instances": {
                        "*": { "texture": "gm1_ord_lf_jump_panel", "render_method": "alpha_test" }
                    }
                }
            }
        ]
    }
}