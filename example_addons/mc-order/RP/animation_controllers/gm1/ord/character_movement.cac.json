                    "animations": [{ "hammer": "!v.is_first_person" }, { "fp_amy_hammer": "v.is_first_person" }]
                }
            }
        },
        "controller.animation.gm1_ord.super_sonic_movement": {
            "initial_state": "none",
            "states": {
                "none": {
                    "animations": [ "bob", "idle_controller" ],
                    "transitions": [
                        {
                            "jog": "(v.is_moving || (v.momentum > 0 && v.momentum < 6)) && (v.is_grounded || v.is_creative_flight)"
                        },
                        { "run": "v.momentum >= 6" },
                        { "jump": "v.jumped_recently && v.secondary_state != 7" },
                        { "ability_charging": "v.ability_state > 0" },
                        { "flight": "v.ability_state == 8" }
                    ],
                    "blend_transition": 0.25
                },
                "jog": {
                    "animations": [ "walk", "bob", "yaw", { "jog_sound": "!v.liquid_running && !v.is_creative_flight" }],
                    "transitions": [
                        { "run": "v.momentum >= 6" },
                        { "none": "!v.is_moving && v.momentum <= 0 && v.is_grounded" },
                        { "jump": "v.jumped_recently && v.secondary_state != 7" },
                        { "ability_charging": "v.ability_state > 0" },
                        { "flight": "v.ability_state == 8" }
                    ],
                    "blend_transition": 0.25
                },
                "run": {
                    "animations": [
                        "run",
                        "yaw",
                        { "run_sound": "!v.liquid_running && !v.is_creative_flight" },
                        { "run_water": "v.water_running && !v.jumped_recently" },
                        { "run_snow": "v.snow_running && !v.jumped_recently" }
                    ],
                    "transitions": [
                        { "none": "!v.is_moving && v.momentum <= 0 && (v.is_grounded || v.is_creative_flight)" },
                        { "jog": "v.momentum < 6 && (v.is_grounded || v.is_creative_flight)" },
                        { "jump": "v.jumped_recently && v.secondary_state != 7" },
                        { "ability_charging": "v.ability_state > 0" },
                        { "flight": "v.ability_state == 8" }
                    ],
                    "blend_transition": 0.25
                },
                "jump": {
                    "animations": [{ "ball": "!v.is_first_person" }, "yaw" ],
                    "transitions": [
                        { "run": "!v.jumped_recently && v.is_moving && (v.is_grounded || v.liquid_running)" },
                        {
                            "jog": "(v.is_moving || (v.momentum > 0 && v.momentum < 6)) && (v.is_grounded || v.liquid_running)"
                        },
                        { "none": "!v.jumped_recently && !v.is_moving && v.is_grounded || v.is_creative_flight" },
                        { "ability_charging": "v.ability_state > 0" },
                        { "flight": "v.ability_state == 8" }
                    ]
                },
                "flight": {
                    "animations": [ "flight", "yaw", { "charging_vfx": "v.is_block_breaking && !v.is_first_person" }],
                    "transitions": [{ "super_hover": "!v.is_moving" }, { "none": "v.ability_state != 8" }],
                    "blend_transition": 0.6
                },
                "super_hover": {
                    "animations": [ "super_hover" ],
                    "transitions": [{ "flight": "v.is_moving" }, { "none": "v.ability_state != 8" }],
                    "blend_transition": 0.4
                },
                "ability_charging": {
                    "transitions": [{ "flight": "v.ability_state == 8" }],
                    "animations": [ "charge", "negate_sneaking_tilt", { "charging_vfx": "v.ability_state == 5" }]
                },
                "ability_released": {
                    "transitions": [{ "roll": "v.time_since_in_ability > 0 " }, { "none": "v.ability_state == 0" }],
                    "animations": [{ "ball": "!v.is_first_person" }, "yaw", { "charging_vfx": "v.is_block_breaking" }]
                },
                "roll": {
                    "animations": [{ "ball": "!v.is_first_person" }, "yaw" ],
                    "transitions": [
                        {
                            "run": "v.momentum >= 6 && v.ability_state < 1 && (v.is_grounded || v.liquid_running) && v.is_moving && v.time_since_in_ability > 0.4"
                        },
                        {
                            "jog": "v.momentum < 6 && v.ability_state < 1 && (v.is_grounded || v.liquid_running) && v.is_moving && v.time_since_in_ability > 0.4"
                        },
                        {
                            "none": "v.ability_state < 1 && (v.is_grounded || v.liquid_running) && !v.is_moving && v.time_since_in_ability > 0.4"
                        },
                        { "jump": "v.jumped_recently && v.secondary_state != 7" }
                    ]
                }
            }
        },
        "controller.animation.gm1_ord.super_shadow_movement": {
            "initial_state": "none",
            "states": {
                "none": {
                    "animations": [ "bob", "idle_controller" ],
                    "transitions": [
                        {
                            "jog": "(v.is_moving || (v.momentum > 0 && v.momentum < 6)) && (v.is_grounded || v.is_creative_flight)"
                        },
                        { "run": "v.momentum >= 6" },
                        { "jump": "v.jumped_recently && v.secondary_state != 7" },
                        { "ability_charging": "v.ability_state > 0" },
                        { "homing_start": "v.secondary_state == 1" },
                        { "flight": "v.ability_state == 8" }
                    ],
                    "blend_transition": 0.25
                },
                "jog": {
                    "animations": [ "walk", "bob", "yaw", { "jog_sound": "!v.liquid_running && !v.is_creative_flight" }],
                    "transitions": [
                        { "run": "v.momentum >= 6" },
                        { "none": "!v.is_moving && v.momentum <= 0 && v.is_grounded" },
                        { "jump": "v.jumped_recently && v.secondary_state != 7" },
                        { "ability_charging": "v.ability_state > 0" },
                        { "homing_start": "v.secondary_state == 1" },
                        { "flight": "v.ability_state == 8" }
                    ],
                    "blend_transition": 0.25
                },
                "run": {
                    "animations": [ "run", "yaw", { "run_sound": "!v.is_creative_flight" }],
                    "transitions": [
                        { "none": "!v.is_moving && v.momentum <= 0 && (v.is_grounded || v.is_creative_flight)" },
                        { "jog": "v.momentum < 6 && (v.is_grounded || v.is_creative_flight)" },
                        { "jump": "v.jumped_recently && v.secondary_state != 7" },
                        { "ability_charging": "v.ability_state > 0" },
                        { "homing_start": "v.secondary_state == 1" },
                        { "flight": "v.ability_state == 8" }
                    ],
                    "blend_transition": 0.25
                },
                "jump": {
                    "animations": [{ "ball": "!v.is_first_person" }, "yaw" ],
                    "transitions": [
                        { "run": "!v.jumped_recently && v.is_moving && (v.is_grounded || v.liquid_running)" },
                        { "none": "!v.jumped_recently && !v.is_moving || v.is_creative_flight" },
                        { "ability_charging": "v.ability_state > 0" },
                        { "homing_start": "v.secondary_state == 1" },
                        { "flight": "v.ability_state == 8" }
                    ]
                },
                "ability_charging": {
                    "transitions": [{ "flight": "v.ability_state == 8" }],
                    "animations": [
                        { "charge": "!v.is_first_person" },
                        { "fp_shadow_charge": "v.is_first_person" },
                        "negate_sneaking_tilt",
                        { "charging_vfx": "v.ability_state == 5" }
                    ]
                },
                "ability_teleport": {
                    "animations": [ "dash_disappear", { "charging_vfx": "v.is_block_breaking" }],
                    "transitions": [
                        { "ability_finished": "q.all_animations_finished" },
                        { "flight": "v.ability_state == 8" }
                    ]
                },
                "ability_finished": {
                    "animations": [ "dash_reappear" ],
                    "transitions": [
                        {
                            "none": "!v.is_moving && v.momentum <= 0 && v.is_grounded && query.any_animation_finished || v.secondary_state == 11"
                        },
                        {
                            "jog": "(v.is_moving || (v.momentum > 0 && v.momentum < 6)) && v.is_grounded && query.any_animation_finished"
                        },
                        { "run": "v.momentum >= 6 && query.any_animation_finished" }
                    ],
                    "blend_transition": 0.25
                },
                "flight": {
                    "animations": [
                        "shadow_flight",
                        "yaw",
                        { "charging_vfx": "v.is_block_breaking && !v.is_first_person" }
                    ],
                    "transitions": [
                        { "super_hover": "!v.is_moving" },
                        { "none": "v.ability_state !=8" },
                        { "homing_start": "v.secondary_state == 1" }
                    ],
                    "blend_transition": 0.4
                },
                "super_hover": {
                    "animations": [ "super_hover" ],
                    "transitions": [{ "flight": "v.is_moving" }, { "none": "v.ability_state !=8 && v.is_grounded" }],
                    "blend_transition": 0.4
                },
                "homing_start": {
                    "animations": [ "snap_disappear" ],
                    "transitions": [
                        { "homing_end": "q.all_animations_finished && !v.secondary_state == 1 && v.ability_state != 8" },
                        {
                            "multi_homing_end": "q.all_animations_finished && !v.secondary_state == 1 && v.ability_state == 8"
                        }
                    ],
                    "on_entry": [ "v.chosen_attack = math.random_integer(1,3);" ]
                },
                "homing_end": {
                    "animations": [
                        { "dash_reappear": "v.chosen_attack == 1" },
                        { "dash_reappear2": "v.chosen_attack == 2" },
                        { "dash_reappear3": "v.chosen_attack == 3" }
                    ],
                    "transitions": [
                        {
                            "none": "v.secondary_state != 1 && !v.is_moving && v.momentum <= 0 && v.is_grounded && query.any_animation_finished"
                        },
                        {
                            "roll": " v.secondary_state != 1 && v.momentum <= 0 && !v.is_grounded && query.any_animation_finished"
                        },
                        {
                            "jog": "v.secondary_state != 1 && (v.is_moving || (v.momentum > 0 && v.momentum < 6)) && v.is_grounded && query.any_animation_finished"
                        },
                        { "run": "v.secondary_state != 1 && v.momentum >= 6 && query.any_animation_finished" },
                        { "homing_start": "v.secondary_state == 1" }
                    ],
                    "blend_transition": 0.25
                },
                "multi_homing_end": {
                    "animations": [
                        { "dash_reappear": "v.chosen_attack == 1" },
                        { "dash_reappear2": "v.chosen_attack == 2" },
                        { "dash_reappear3": "v.chosen_attack == 3" }
                    ],
                    "transitions": [
                        {
                            "none": "v.secondary_state != 1 && v.ability_state != 8 && !v.is_moving && v.momentum <= 0 && v.is_grounded && query.any_animation_finished"
                        },
                        {
                            "roll": " v.secondary_state != 1 && v.ability_state != 8 && v.momentum <= 0 && !v.is_grounded && query.any_animation_finished"
                        },
                        {
                            "jog": "v.secondary_state != 1 && v.ability_state != 8 && (v.is_moving || (v.momentum > 0 && v.momentum < 6)) && v.is_grounded && query.any_animation_finished"
                        },
                        {
                            "run": "v.secondary_state != 1 && v.ability_state != 8 && v.momentum >= 6 && query.any_animation_finished"
                        },
                        { "homing_start": "v.secondary_state == 1" },
                        {
                            "flight": "v.secondary_state != 1 && v.ability_state == 8 && query.any_animation_finished && query.state_time > 0.45"
                        }
                    ]
                },
                "roll": {
                    "animations": [{ "ball": "!v.is_first_person" }, "yaw" ],
                    "transitions": [
                        { "run": "!v.jumped_recently && v.is_moving && (v.is_grounded || v.liquid_running)" },
                        { "none": "!v.jumped_recently && !v.is_moving && v.is_grounded || v.is_creative_flight" },
                        { "ability_charging": "v.ability_state > 0" },
                        { "homing_start": "v.secondary_state == 1" }
                    ],
                    "blend_transition": 0.25,
                    "blend_via_shortest_path": true
                }
            }
        },
        "controller.animation.gm1_ord.blink": {
            "initial_state": "none",
            "states": {
                "none": {
                    "animations": [ "eyes_open" ],
                    "transitions": [{ "blink": "math.die_roll_integer(1, 0, 999)>996" }],
