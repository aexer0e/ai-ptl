{
    "$templates": [{ "$template": "ord:character_controller", "character": 3, "character_string": "super_shadow" }],
    "format_version": "1.10.0",
    "minecraft:attachable": {
        "description": {
            "identifier": "gm1_ord:transform_helmet_super_shadow",
            "textures": {
                "super_aura_1": "textures/gm1/ord/entity/aura/super_shadow_aura_1",
                "super_aura_2": "textures/gm1/ord/entity/aura/super_shadow_aura_2",
                "super_aura_3": "textures/gm1/ord/entity/aura/super_shadow_aura_3",
                "super_aura_4": "textures/gm1/ord/entity/aura/super_shadow_aura_4",
                "super_aura_5": "textures/gm1/ord/entity/aura/super_shadow_aura_5",
                "super_aura_6": "textures/gm1/ord/entity/aura/super_shadow_aura_6",
                "super_aura_7": "textures/gm1/ord/entity/aura/super_shadow_aura_7",
                "super_aura_8": "textures/gm1/ord/entity/aura/super_shadow_aura_8",
                "super_aura_9": "textures/gm1/ord/entity/aura/super_shadow_aura_9",
                "super_aura_10": "textures/gm1/ord/entity/aura/super_shadow_aura_10",
                "super_aura_11": "textures/gm1/ord/entity/aura/super_shadow_aura_11",
                "super_aura_12": "textures/gm1/ord/entity/aura/super_shadow_aura_12",
                "super_aura_13": "textures/gm1/ord/entity/aura/super_shadow_aura_13",
                "super_aura_14": "textures/gm1/ord/entity/aura/super_shadow_aura_14",
                "super_aura_15": "textures/gm1/ord/entity/aura/super_shadow_aura_15",
                "super_aura_16": "textures/gm1/ord/entity/aura/super_shadow_aura_16"
            },
            "geometry": {
                "super_shell": "geometry.gm1_ord.character_super_shadow_charge_shell",
                "super_aura": "geometry.gm1_ord.super_aura"
            },
            "animations": {
                "snap_disappear": "animation.gm1_ord.character_shadow.snap_disappear",
                "snap_reappear": "animation.gm1_ord.character_shadow.snap_reappear",
                "dash_disappear": "animation.gm1_ord.character_shadow.dash_disappear",
                "dash_reappear": "animation.gm1_ord.character_shadow.dash_stop",
                "dash_reappear2": "animation.gm1_ord.character_shadow.dash_stop2",
                "dash_reappear3": "animation.gm1_ord.character_shadow.dash_stop3",
                "charging.chaos1": "animation.gm1_ord.shadow.chaos1_sound",
                "charging.chaos2": "animation.gm1_ord.shadow.chaos2_sound",
                "charging.chaos3": "animation.gm1_ord.shadow.chaos3_sound",
                "charging.chaos4": "animation.gm1_ord.shadow.chaos4_sound",
                "charging.chaos5": "animation.gm1_ord.shadow.chaos5_sound",
                "super_shadow_boost_sfx": "animation.gm1_ord.super_shadow.boost_sfx",
                "shadow.blink_in": "animation.gm1_ord.shadow.blink_in",
                "twirl": "animation.gm1_ord.transform_twirl.supershadow",
                "super_aura_rotation": "animation.gm1_ord.super_aura.rot_to_camera",
                "super_transform_controller": "controller.animation.gm1_ord.super_transform",
                "super_loop_sfx": "animation.gm1_ord.super_form.loop_sfx"
            },
            "sound_effects": {
                "chaos1": "shadow_charge1",
                "chaos2": "shadow_charge2",
                "chaos3": "shadow_charge3",
                "chaos4": "shadow_charge4",
                "chaos5": "shadow_charge5",
                "super_shadow_boost_sfx": "super_shadow_boost",
                "super_loop_sfx": "super_loop",
                "shadow_blink_in": "shadow_blink_in",
                "shadow_step": "shadow_step"
            },
            "scripts": {
                "initialize": [ "v.super_aura = 0;", "v.is_super = 1;" ],
                "pre_animation": [
                    "v.super_aura = v.super_aura + 16 * query.delta_time;",
                    "v.super_aura = v.super_aura >= 16 ? 0 : v.super_aura;"
                ],
                "animate": [
                    {
                        "movement_controller": "!v.is_first_person && v.has_transform_helmet && v.time_as_character >= 3.0"
                    },
                    { "super_transform_controller": "!v.is_first_person" },
                    { "detransform_controller": "!v.is_first_person" },
                    { "ability_charge_controller": "!v.is_first_person" },
                    { "blink_controller": "!v.is_first_person" },
                    { "target_eyes": "!v.is_first_person" },
                    { "flycam": "v.flycam_turned_on && !q.is_in_ui && !v.is_first_person" },
                    { "body_direction": "v.flycam_turned_on && !q.is_in_ui && !v.is_first_person" },
                    { "top_speed.sfx": "!v.is_first_person" },
                    { "trail_scale": "!v.is_first_person" },
                    { "liquid_running_offset_controller": "!v.is_first_person" },
                    { "liquid_running_controller": "!v.is_first_person" },
                    { "jump_effects_controller": "!v.is_first_person && v.has_transform_helmet" },
                    "super_aura_rotation",
                    { "super_loop_sfx": "!v.is_first_person && v.has_transform_helmet" }
                ]
            },
            "render_controllers": [
                {
                    "controller.render.gm1_ord.character": "v.character <= 1 ? !v.is_first_person && v.is_transformed && !v.detransforming && !v.ballmode : !v.is_first_person && v.is_transformed && !v.detransforming"
                },
                {
                    "controller.render.gm1_ord.character_charging": "!v.is_first_person && v.is_transformed && (v.ability_state > 0 && v.ability_state <= 5)"
                },
                {
                    "controller.render.gm1_ord.character_transforming": "v.transforming || v.detransforming || v.is_losing_life"
                },
                { "controller.render.gm1_ord.trail_flipbook": "!v.is_first_person" },
                {
                    "controller.render.gm1_ord.super_shell": "v.secondary_state != 1 && (v.ability_state <= 0 || v.ability_state > 5) && !v.is_first_person && v.is_transformed && !v.detransforming && (!v.ballmode || v.time_since_ability_active) "
                },
                {
                    "controller.render.gm1_ord.super_aura": "v.secondary_state != 1 && !v.is_first_person && v.is_transformed && !v.detransforming && (!v.ballmode || v.time_since_ability_active)"
                },
                { "controller.render.gm1_ord.shadow_home": "!v.is_first_person && v.secondary_state == 1" }
            ]
        }
    }
}