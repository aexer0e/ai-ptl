{
    "format_version": "1.8.0",
    "animations": {
        "animation.gm1_ord.ring.rotate": {
            "loop": true,
            "animation_length": 1,
            "bones": {
                "root": { "rotation": { "0.0": [ 0, 0, 0 ], "1.0": [ 0, 360, 0 ]}}
            }
        },
        "animation.gm1_ord.ring.fast_rotate": {
            "loop": true,
            "bones": {
                "root": { "rotation": [ 0, "!query.is_on_ground*(query.anim_time*1080)", 0 ]}
            }
        },
        "animation.gm1_ord.ring.scale_up": {
            "loop": "hold_on_last_frame",
            "animation_length": 1,
            "bones": {
                "ring": { "scale": { "0.0": [ 0, 0, 0 ], "0.6": [ 1, 1, 1 ]}}
            }
        },
        "animation.gm1_ord.ring.spawn": {
            "loop": "hold_on_last_frame",
            "animation_length": 1,
            "bones": {
                "root": {
                    "rotation": { "0.0": [ 0, "query.anim_time*360", 0 ], "1.0": [ 0, "query.anim_time*720", 0 ]},
                    "scale": {
                        "0.0": { "post": [ 0, 0, 0 ], "lerp_mode": "catmullrom" },
                        "0.4583": { "post": [ 1.7, 1.7, 1.7 ], "lerp_mode": "catmullrom" },
                        "0.6667": { "post": [ 0.9, 0.9, 0.9 ], "lerp_mode": "catmullrom" },
                        "1.0": [ 1, 1, 1 ]
                    }
                }
            }
        },
        "animation.gm1_ord.ring.collect": {
            "animation_length": 1,
            "loop": false,
            "particle_effects": { "0.0": [{ "effect": "collection" }]},
            "sound_effects": { "0.0": [{ "effect": "ring_collect_sound" }]}
        }
    }
}