{
    "format_version": "1.8.0",
    "animations": {
        "animation.gm1_ord.warp_ring.rotate": {
            "loop": true,
            "animation_length": 2,
            "bones": {
                "root": {
                    "rotation": { "0.0": [ 0, 0, 0 ], "2.0": [ 0, 360, 0 ]},
                    "position": [ 0, "math.sin(q.anim_time*180)*1", 0 ]
                }
            }
        },
        "animation.gm1_ord.warp_ring.spawn": {
            "loop": "hold_on_last_frame",
            "animation_length": 1,
            "bones": {
                "root": {
                    "rotation": { "0.0": [ 0, "query.anim_time*360", 0 ], "1.0": [ 0, "query.anim_time*720", 0 ]},
                    "scale": {
                        "0.0": { "post": [ 0, 0, 0 ], "lerp_mode": "catmullrom" },
                        "0.4583": { "post": [ 1.7, 1.7, 1.7 ], "lerp_mode": "catmullrom" },
                        "0.6667": { "post": [ 0.9, 0.9, 0.9 ], "lerp_mode": "catmullrom" },
                        "1.0": [ 1, 1, 1 ]
                    }
                }
            }
        },
        "animation.gm1_ord.warp_ring.face_player": {
            "loop": true,
            "bones": {
                "root": {
                    "rotation": [ 0, "query.rotation_to_camera(1)- query.body_y_rotation", 0 ],
                    "position": [ 0, "math.sin(q.anim_time*180)*1", 0 ]
                }
            }
        }
    }
}