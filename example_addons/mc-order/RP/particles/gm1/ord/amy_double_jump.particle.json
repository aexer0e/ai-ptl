                        "step_UV": [ 16, 0 ],
                        "frames_per_second": 3,
                        "max_frame": 5,
                        "stretch_to_lifetime": true
                    }
                }
            },
            "minecraft:particle_appearance_tinting": {
                "color": {
                    "interpolant": "variable.particle_age/variable.particle_lifetime",
                    "gradient": { "0.11": "#FFFFFFFF", "0.69": "#FFFF61CF", "1.0": "#00FFFFFF" }
                }
            }
        }
    }
