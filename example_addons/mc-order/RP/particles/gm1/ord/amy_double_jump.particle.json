{
    "format_version": "1.10.0",
    "particle_effect": {
        "description": {
            "identifier": "gm1_ord:amy_double_jump",
            "basic_render_parameters": { "material": "particles_blend", "texture": "textures/gm1/ord/particle/poof" }
        },
        "curves": {
            "variable.size": {
                "type": "linear",
                "input": "variable.particle_age/variable.particle_lifetime",
                "horizontal_range": 1,
                "nodes": [ 0.3, 0.3, 0 ]
            },
            "variable.spawn": {
                "type": "bezier_chain",
                "input": "v.particle_age",
                "nodes": {
                    "0.0": { "value": 0, "slope": 0.7 },
                    "1.0": { "value": -1.34, "slope": -1.05 },
                    "0.07": { "value": 0.57, "slope": -1.05 },
                    "0.16": { "value": -0.3, "slope": -0.4 }
                }
            }
        },
        "components": {
            "minecraft:emitter_rate_instant": { "num_particles": 20 },
            "minecraft:emitter_lifetime_once": { "active_time": 1 },
            "minecraft:emitter_shape_disc": {
                "offset": [ 0, "math.random(-0.050,0.050)", 0 ],
                "radius": 0.01,
                "direction": "outwards"
            },
            "minecraft:particle_lifetime_expression": { "max_lifetime": "Math.random(0.75, 1)" },
